import java.io.*;
import java.util.Scanner;
class Area
{
int r;
float  pi=3.14f,result;
void display(int r1)
{
r=r1;
result = pi*r*r;
System.out.println(result);
}
}
class Areaofcircle
{
public static void main(String[] args)
{
Area a1 = new Area();
Scanner input = new Scanner(System.in);
System.out.println("Enter the radius: ");
a1.r =input.nextInt();
a1.display(a1.r);
}
}
